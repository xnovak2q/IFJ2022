<?php
declare(strict_types=1);

write(,);

function intFoo(int $abc): int{
    if($abc){
        return $abc;
    } else { $b = $abc;}

    return $b;
}

function no(int $ok): void {
}

if(1){
    no(1);

    if(1){
        no(1);
    } else {
        $a = intFoo(1);
    }
} else {
    no(2);
}

$a = intFoo(2);
intFoo($a);

no(1); 